{
  "include": ["src"],
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["ESNext"],
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "sourceMap": true
  }
}
